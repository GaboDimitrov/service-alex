{
    "name": "service-alex",
    "version": "1.0.0",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "start": "node server.js",
        "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "aws-sdk": "^2.353.0",
        "axios": "^0.18.0",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.4.0",
        "jwt-simple": "^0.5.5",
        "mongoose": "^5.3.11",
        "node-cron": "^2.0.3",
        "nodemon": "^1.18.6",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "react": "^16.6.3",
        "react-bootstrap": "^0.32.4",
        "react-css-modules": "^4.7.7",
        "react-dom": "^16.6.1",
        "react-notifications": "^1.4.3",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.4.0-beta.6",
        "react-router5": "^6.5.3",
        "webpack": "4.19.1"
    },
    "devDependencies": {
        "concurrently": "^4.0.1"
    }
